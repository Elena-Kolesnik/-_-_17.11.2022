// Задача 6: Напишите программу, которая на вход принимает число и выдаёт, является ли число чётным (делится ли оно на два без остатка).
// 4 -> да
// -3 -> нет
// 7 -> нет


using static System.Console;
Clear();


int number = int.Parse(ReadLine());

if (number % 2 == 0)
{ 
    WriteLine("да");
}
else
{
    WriteLine("нет");
}  
